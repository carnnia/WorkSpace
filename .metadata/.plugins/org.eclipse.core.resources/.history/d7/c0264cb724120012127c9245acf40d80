
public class HelloWorld {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		System.out.println("Hello World!?!");
		System.out.println("number of args " + args.length);
		
		for (int i = 0; i < args.length; i++) {
			System.out.println(args[i]);  
		}//for END
	}//main END
}//class END
